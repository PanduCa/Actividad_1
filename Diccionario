meme_dict = {
            "CRINGE": "Algo excepcionalmente raro o embarazoso",
            "LOL": "Una respuesta común a algo gracioso",
            "ROFL": "una respuesta a una broma",
            "SHEESH": "ligera desaprobación",
            "CREEPY": "aterrador, siniestro",
            "AGGRO": "ponerse agresivo/enojado"
            }
diccionario = ["CRINGE", "LOL", "ROFL", "SHEESH", "CREEPY", "AGGRO"]
print("El diccionario de hoy es:", diccionario[-6], ",", diccionario[-5], ",", diccionario[-4], ",", diccionario[-3], ",", diccionario[-2], ",", diccionario[-1])
word = input("Escribe una palabra que no entiendas (¡con mayúsculas!): ")
if word in meme_dict.keys():
    print("Definición:", meme_dict[word])
else:
    print("Ingresa una palabra dentro del diccionario, asegurate que este en mayúsuclas")
    
word1 = input("Alguna otra palabra que quisieras saber?")
if word1 in meme_dict.keys():
    print("Definición:", meme_dict[word1])
else:
    print("Ingresa una palabra dentro del diccionario, asegurate que este en mayúsuclas")
    
